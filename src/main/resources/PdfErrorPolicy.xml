        shouldWarn="false"/>
    <PdfError name="ERROR_SYNTAX_NOCATALOG" code="1.2.13" shouldFail="true" shouldWarn="false"/>
    <!--
Common error about the cross ref table
-->
    <PdfError name="ERROR_SYNTAX_CROSS_REF" code="1.3" shouldFail="true" shouldWarn="false"/>
    <!--
Common error about the trailer
-->
    <PdfError name="ERROR_SYNTAX_TRAILER" code="1.4" shouldFail="true" shouldWarn="false"/>
