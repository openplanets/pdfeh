An object is missing from the document
-->
    <PdfError name="ERROR_SYNTAX_MISSING_OFFSET" code="1.0.13" shouldFail="true" shouldWarn="false"/>
    <!--
An object has an invalid offset
-->
    <PdfError name="ERROR_SYNTAX_INVALID_OFFSET" code="1.0.14" shouldFail="true" shouldWarn="false"/>
