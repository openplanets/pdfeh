        set shouldFail to "false" and shouldWarn to "true" in the PdfError element for that error code-->
    <!-- Error code for syntax error -->
    <PdfError name="ERROR_SYNTAX_MAIN" code="1" shouldFail="true" shouldWarn="false"/>
    <!--
        error code category which can occur in each pdf part
    -->
    <PdfError name="ERROR_SYNTAX_COMMON" code="1.0" shouldFail="true" shouldWarn="false"/>
    <!--
         Too many entries in a dictionary object
    -->
    <PdfError name="ERROR_SYNTAX_TOO_MANY_ENTRIES" code="1.0.1" shouldFail="true" shouldWarn="false"/>
    <!--
        Too many element in an array object
    -->
    <PdfError name="ERROR_SYNTAX_ARRAY_TOO_LONG" code="1.0.2" shouldFail="true" shouldWarn="false"/>
    <!--
        The name length is too long
    -->
    <PdfError name="ERROR_SYNTAX_NAME_TOO_LONG" code="1.0.3" shouldFail="true" shouldWarn="false"/>
    <!--
The literal string is too long
-->
    <PdfError name="ERROR_SYNTAX_LITERAL_TOO_LONG" code="1.0.4" shouldFail="true" shouldWarn="false"/>
