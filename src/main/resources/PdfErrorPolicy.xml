ID is missing from the trailer
-->
    <PdfError name="ERROR_SYNTAX_TRAILER_MISSING_ID" code="1.4.1" shouldFail="true"
        shouldWarn="false"/>
    <!--
Encrypt is forbidden
-->
    <PdfError name="ERROR_SYNTAX_TRAILER_ENCRYPT" code="1.4.2" shouldFail="true" shouldWarn="false"/>
    <!--
An trailer entry has an invalid type
-->
    <PdfError name="ERROR_SYNTAX_TRAILER_TYPE_INVALID" code="1.4.3" shouldFail="true"
        shouldWarn="false"/>
    <!--
Size is missing from the trailer
-->
    <PdfError name="ERROR_SYNTAX_TRAILER_MISSING_SIZE" code="1.4.4" shouldFail="true"
        shouldWarn="false"/>
    <!--
Root is missing from the trailer
-->
    <PdfError name="ERROR_SYNTAX_TRAILER_MISSING_ROOT" code="1.4.5" shouldFail="true"
        shouldWarn="false"/>
